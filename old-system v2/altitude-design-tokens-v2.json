{
  "$schema": "https://schemas.tokens.studio/0.1/tokens.json",
  "$metadata": {
    "name": "Altitude Design System",
    "version": "1.0.0",
    "description": "Framework-agnostic design tokens for Figma, Greenlight/WordPress, and Elementor",
    "baseUnit": "16px",
    "gridSystem": "4px/8px",
    "mobileViewport": "375px",
    "desktopViewport": "1440px"
  },

  "primitives": {
    "spacing": {
      "$description": "4px/8px grid spacing scale (Tailwind-style naming)",
      "0": { "$value": "0rem", "$type": "dimension", "px": "0px", "wpVar": "--wp--preset--spacing--0" },
      "px": { "$value": "0.0625rem", "$type": "dimension", "px": "1px", "wpVar": "--wp--preset--spacing--px" },
      "0.5": { "$value": "0.125rem", "$type": "dimension", "px": "2px", "wpVar": "--wp--preset--spacing--0-5" },
      "1": { "$value": "0.25rem", "$type": "dimension", "px": "4px", "wpVar": "--wp--preset--spacing--1" },
      "1.5": { "$value": "0.375rem", "$type": "dimension", "px": "6px", "wpVar": "--wp--preset--spacing--1-5" },
      "2": { "$value": "0.5rem", "$type": "dimension", "px": "8px", "wpVar": "--wp--preset--spacing--2" },
      "2.5": { "$value": "0.625rem", "$type": "dimension", "px": "10px", "wpVar": "--wp--preset--spacing--2-5" },
      "3": { "$value": "0.75rem", "$type": "dimension", "px": "12px", "wpVar": "--wp--preset--spacing--3" },
      "3.5": { "$value": "0.875rem", "$type": "dimension", "px": "14px", "wpVar": "--wp--preset--spacing--3-5" },
      "4": { "$value": "1rem", "$type": "dimension", "px": "16px", "wpVar": "--wp--preset--spacing--4" },
      "5": { "$value": "1.25rem", "$type": "dimension", "px": "20px", "wpVar": "--wp--preset--spacing--5" },
      "6": { "$value": "1.5rem", "$type": "dimension", "px": "24px", "wpVar": "--wp--preset--spacing--6" },
      "7": { "$value": "1.75rem", "$type": "dimension", "px": "28px", "wpVar": "--wp--preset--spacing--7" },
      "8": { "$value": "2rem", "$type": "dimension", "px": "32px", "wpVar": "--wp--preset--spacing--8" },
      "9": { "$value": "2.25rem", "$type": "dimension", "px": "36px", "wpVar": "--wp--preset--spacing--9" },
      "10": { "$value": "2.5rem", "$type": "dimension", "px": "40px", "wpVar": "--wp--preset--spacing--10" },
      "12": { "$value": "3rem", "$type": "dimension", "px": "48px", "wpVar": "--wp--preset--spacing--12" },
      "14": { "$value": "3.5rem", "$type": "dimension", "px": "56px", "wpVar": "--wp--preset--spacing--14" },
      "16": { "$value": "4rem", "$type": "dimension", "px": "64px", "wpVar": "--wp--preset--spacing--16" },
      "20": { "$value": "5rem", "$type": "dimension", "px": "80px", "wpVar": "--wp--preset--spacing--20" },
      "24": { "$value": "6rem", "$type": "dimension", "px": "96px", "wpVar": "--wp--preset--spacing--24" },
      "32": { "$value": "8rem", "$type": "dimension", "px": "128px", "wpVar": "--wp--preset--spacing--32" },
      "40": { "$value": "10rem", "$type": "dimension", "px": "160px", "wpVar": "--wp--preset--spacing--40" },
      "48": { "$value": "12rem", "$type": "dimension", "px": "192px", "wpVar": "--wp--preset--spacing--48" },
      "56": { "$value": "14rem", "$type": "dimension", "px": "224px", "wpVar": "--wp--preset--spacing--56" },
      "64": { "$value": "16rem", "$type": "dimension", "px": "256px", "wpVar": "--wp--preset--spacing--64" }
    },

    "fontSize": {
      "$description": "Typography scales with Major Second (1.125) and Major Third (1.25)",
      "majorSecond": {
        "2xs": {
          "clamp": { "$value": "clamp(0.625rem, 0.56rem + 0.28vw, 0.75rem)", "$type": "dimension" },
          "mobile": { "$value": "10px", "$type": "dimension" },
          "desktop": { "$value": "12px", "$type": "dimension" },
          "semantic": "caption, overline"
        },
        "xs": {
          "clamp": { "$value": "clamp(0.75rem, 0.68rem + 0.31vw, 0.875rem)", "$type": "dimension" },
          "mobile": { "$value": "12px", "$type": "dimension" },
          "desktop": { "$value": "14px", "$type": "dimension" },
          "semantic": "small, label"
        },
        "sm": {
          "clamp": { "$value": "clamp(0.875rem, 0.81rem + 0.35vw, 1rem)", "$type": "dimension" },
          "mobile": { "$value": "14px", "$type": "dimension" },
          "desktop": { "$value": "16px", "$type": "dimension" },
          "semantic": "body-small"
        },
        "md": {
          "clamp": { "$value": "clamp(1rem, 0.93rem + 0.39vw, 1.125rem)", "$type": "dimension" },
          "mobile": { "$value": "16px", "$type": "dimension" },
          "desktop": { "$value": "18px", "$type": "dimension" },
          "semantic": "body, paragraph"
        },
        "lg": {
          "clamp": { "$value": "clamp(1.125rem, 1.04rem + 0.44vw, 1.25rem)", "$type": "dimension" },
          "mobile": { "$value": "18px", "$type": "dimension" },
          "desktop": { "$value": "20px", "$type": "dimension" },
          "semantic": "lead, intro"
        },
        "xl": {
          "clamp": { "$value": "clamp(1.25rem, 1.16rem + 0.49vw, 1.5rem)", "$type": "dimension" },
          "mobile": { "$value": "20px", "$type": "dimension" },
          "desktop": { "$value": "24px", "$type": "dimension" },
          "semantic": "h6, subheading"
        },
        "2xl": {
          "clamp": { "$value": "clamp(1.5rem, 1.39rem + 0.56vw, 1.75rem)", "$type": "dimension" },
          "mobile": { "$value": "24px", "$type": "dimension" },
          "desktop": { "$value": "28px", "$type": "dimension" },
          "semantic": "h5"
        },
        "3xl": {
          "clamp": { "$value": "clamp(1.75rem, 1.62rem + 0.63vw, 2rem)", "$type": "dimension" },
          "mobile": { "$value": "28px", "$type": "dimension" },
          "desktop": { "$value": "32px", "$type": "dimension" },
          "semantic": "h4"
        },
        "4xl": {
          "clamp": { "$value": "clamp(2rem, 1.85rem + 0.7vw, 2.25rem)", "$type": "dimension" },
          "mobile": { "$value": "32px", "$type": "dimension" },
          "desktop": { "$value": "36px", "$type": "dimension" },
          "semantic": "h3"
        },
        "5xl": {
          "clamp": { "$value": "clamp(2.25rem, 2.08rem + 0.78vw, 2.5rem)", "$type": "dimension" },
          "mobile": { "$value": "36px", "$type": "dimension" },
          "desktop": { "$value": "40px", "$type": "dimension" },
          "semantic": "h2"
        },
        "6xl": {
          "clamp": { "$value": "clamp(2.5rem, 2.31rem + 0.85vw, 3rem)", "$type": "dimension" },
          "mobile": { "$value": "40px", "$type": "dimension" },
          "desktop": { "$value": "48px", "$type": "dimension" },
          "semantic": "h1, hero"
        }
      },
      "majorThird": {
        "2xs": {
          "clamp": { "$value": "clamp(0.5rem, 0.43rem + 0.39vw, 0.8rem)", "$type": "dimension" },
          "mobile": { "$value": "8px", "$type": "dimension" },
          "desktop": { "$value": "13px", "$type": "dimension" },
          "semantic": "caption, overline"
        },
        "xs": {
          "clamp": { "$value": "clamp(0.625rem, 0.54rem + 0.49vw, 1rem)", "$type": "dimension" },
          "mobile": { "$value": "10px", "$type": "dimension" },
          "desktop": { "$value": "16px", "$type": "dimension" },
          "semantic": "small, label"
        },
        "sm": {
          "clamp": { "$value": "clamp(0.8rem, 0.68rem + 0.61vw, 1.25rem)", "$type": "dimension" },
          "mobile": { "$value": "13px", "$type": "dimension" },
          "desktop": { "$value": "20px", "$type": "dimension" },
          "semantic": "body-small"
        },
        "md": {
          "clamp": { "$value": "clamp(1rem, 0.85rem + 0.76vw, 1.56rem)", "$type": "dimension" },
          "mobile": { "$value": "16px", "$type": "dimension" },
          "desktop": { "$value": "25px", "$type": "dimension" },
          "semantic": "body, paragraph"
        },
        "lg": {
          "clamp": { "$value": "clamp(1.25rem, 1.06rem + 0.95vw, 1.95rem)", "$type": "dimension" },
          "mobile": { "$value": "20px", "$type": "dimension" },
          "desktop": { "$value": "31px", "$type": "dimension" },
          "semantic": "lead, intro"
        },
        "xl": {
          "clamp": { "$value": "clamp(1.56rem, 1.33rem + 1.19vw, 2.44rem)", "$type": "dimension" },
          "mobile": { "$value": "25px", "$type": "dimension" },
          "desktop": { "$value": "39px", "$type": "dimension" },
          "semantic": "h6, subheading"
        },
        "2xl": {
          "clamp": { "$value": "clamp(1.95rem, 1.66rem + 1.48vw, 3.05rem)", "$type": "dimension" },
          "mobile": { "$value": "31px", "$type": "dimension" },
          "desktop": { "$value": "49px", "$type": "dimension" },
          "semantic": "h5"
        },
        "3xl": {
          "clamp": { "$value": "clamp(2.44rem, 2.08rem + 1.85vw, 3.81rem)", "$type": "dimension" },
          "mobile": { "$value": "39px", "$type": "dimension" },
          "desktop": { "$value": "61px", "$type": "dimension" },
          "semantic": "h4"
        },
        "4xl": {
          "clamp": { "$value": "clamp(3.05rem, 2.6rem + 2.31vw, 4.77rem)", "$type": "dimension" },
          "mobile": { "$value": "49px", "$type": "dimension" },
          "desktop": { "$value": "76px", "$type": "dimension" },
          "semantic": "h3"
        },
        "5xl": {
          "clamp": { "$value": "clamp(3.81rem, 3.25rem + 2.89vw, 5.96rem)", "$type": "dimension" },
          "mobile": { "$value": "61px", "$type": "dimension" },
          "desktop": { "$value": "95px", "$type": "dimension" },
          "semantic": "h2"
        },
        "6xl": {
          "clamp": { "$value": "clamp(4.77rem, 4.06rem + 3.61vw, 7.45rem)", "$type": "dimension" },
          "mobile": { "$value": "76px", "$type": "dimension" },
          "desktop": { "$value": "119px", "$type": "dimension" },
          "semantic": "h1, hero"
        }
      }
    },

    "layoutSize": {
      "$description": "Container and layout width tokens",
      "xs": { "$value": "20rem", "$type": "dimension", "px": "320px", "use": "Mobile width" },
      "sm": { "$value": "24rem", "$type": "dimension", "px": "384px", "use": "Small container" },
      "md": { "$value": "28rem", "$type": "dimension", "px": "448px", "use": "Medium container" },
      "lg": { "$value": "32rem", "$type": "dimension", "px": "512px", "use": "Large container" },
      "xl": { "$value": "36rem", "$type": "dimension", "px": "576px", "use": "Extra large" },
      "2xl": { "$value": "42rem", "$type": "dimension", "px": "672px", "use": "Prose/reading width" },
      "3xl": { "$value": "48rem", "$type": "dimension", "px": "768px", "use": "Tablet width" },
      "4xl": { "$value": "56rem", "$type": "dimension", "px": "896px", "use": "Medium desktop" },
      "5xl": { "$value": "64rem", "$type": "dimension", "px": "1024px", "use": "Default content" },
      "6xl": { "$value": "72rem", "$type": "dimension", "px": "1152px", "use": "Wide content" },
      "7xl": { "$value": "80rem", "$type": "dimension", "px": "1280px", "use": "Max content" }
    },

    "size": {
      "$description": "Control and element size tokens",
      "4": { "$value": "1rem", "$type": "dimension", "px": "16px", "use": "Icon small" },
      "5": { "$value": "1.25rem", "$type": "dimension", "px": "20px", "use": "Icon default" },
      "6": { "$value": "1.5rem", "$type": "dimension", "px": "24px", "use": "Icon large" },
      "8": { "$value": "2rem", "$type": "dimension", "px": "32px", "use": "Avatar xs" },
      "10": { "$value": "2.5rem", "$type": "dimension", "px": "40px", "use": "Control height sm" },
      "12": { "$value": "3rem", "$type": "dimension", "px": "48px", "use": "Control height md" },
      "14": { "$value": "3.5rem", "$type": "dimension", "px": "56px", "use": "Control height lg" },
      "16": { "$value": "4rem", "$type": "dimension", "px": "64px", "use": "Avatar md" },
      "20": { "$value": "5rem", "$type": "dimension", "px": "80px", "use": "Avatar lg" },
      "24": { "$value": "6rem", "$type": "dimension", "px": "96px", "use": "Feature icon" },
      "32": { "$value": "8rem", "$type": "dimension", "px": "128px", "use": "Thumbnail sm" },
      "40": { "$value": "10rem", "$type": "dimension", "px": "160px", "use": "Thumbnail md" },
      "48": { "$value": "12rem", "$type": "dimension", "px": "192px", "use": "Card image" }
    },

    "radius": {
      "$description": "Border radius scale following 4px grid",
      "none": { "$value": "0rem", "$type": "dimension", "px": "0px" },
      "sm": { "$value": "0.125rem", "$type": "dimension", "px": "2px" },
      "md": { "$value": "0.25rem", "$type": "dimension", "px": "4px" },
      "lg": { "$value": "0.375rem", "$type": "dimension", "px": "6px" },
      "xl": { "$value": "0.5rem", "$type": "dimension", "px": "8px" },
      "2xl": { "$value": "0.75rem", "$type": "dimension", "px": "12px" },
      "3xl": { "$value": "1rem", "$type": "dimension", "px": "16px" },
      "4xl": { "$value": "1.5rem", "$type": "dimension", "px": "24px" },
      "full": { "$value": "9999px", "$type": "dimension", "px": "9999px" }
    },

    "shadow": {
      "$description": "Elevation shadow scale",
      "xs": { "$value": "0 1px 2px 0 rgb(0 0 0 / 0.05)", "$type": "boxShadow" },
      "sm": { "$value": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)", "$type": "boxShadow" },
      "md": { "$value": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)", "$type": "boxShadow" },
      "lg": { "$value": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)", "$type": "boxShadow" },
      "xl": { "$value": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)", "$type": "boxShadow" },
      "2xl": { "$value": "0 25px 50px -12px rgb(0 0 0 / 0.25)", "$type": "boxShadow" },
      "inner": { "$value": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)", "$type": "boxShadow" }
    },

    "duration": {
      "$description": "Animation duration tokens",
      "75": { "$value": "75ms", "$type": "duration" },
      "100": { "$value": "100ms", "$type": "duration" },
      "150": { "$value": "150ms", "$type": "duration" },
      "200": { "$value": "200ms", "$type": "duration" },
      "300": { "$value": "300ms", "$type": "duration" },
      "500": { "$value": "500ms", "$type": "duration" },
      "700": { "$value": "700ms", "$type": "duration" },
      "1000": { "$value": "1000ms", "$type": "duration" }
    },

    "ease": {
      "$description": "Animation easing tokens",
      "linear": { "$value": "linear", "$type": "cubicBezier" },
      "in": { "$value": "cubic-bezier(0.4, 0, 1, 1)", "$type": "cubicBezier" },
      "out": { "$value": "cubic-bezier(0, 0, 0.2, 1)", "$type": "cubicBezier" },
      "in-out": { "$value": "cubic-bezier(0.4, 0, 0.2, 1)", "$type": "cubicBezier" },
      "bounce": { "$value": "cubic-bezier(0.68, -0.55, 0.265, 1.55)", "$type": "cubicBezier" }
    },

    "lineHeight": {
      "none": { "$value": "1", "$type": "number" },
      "tight": { "$value": "1.25", "$type": "number" },
      "snug": { "$value": "1.375", "$type": "number" },
      "normal": { "$value": "1.5", "$type": "number" },
      "relaxed": { "$value": "1.625", "$type": "number" },
      "loose": { "$value": "2", "$type": "number" }
    },

    "fontWeight": {
      "thin": { "$value": "100", "$type": "fontWeight" },
      "light": { "$value": "300", "$type": "fontWeight" },
      "normal": { "$value": "400", "$type": "fontWeight" },
      "medium": { "$value": "500", "$type": "fontWeight" },
      "semibold": { "$value": "600", "$type": "fontWeight" },
      "bold": { "$value": "700", "$type": "fontWeight" },
      "extrabold": { "$value": "800", "$type": "fontWeight" },
      "black": { "$value": "900", "$type": "fontWeight" }
    }
  },

  "semantic": {
    "color": {
      "$description": "Semantic color tokens that map to Greenlight presets",
      "brand": { "$value": "{wp.preset.color.brand}", "wpVar": "--wp--preset--color--brand", "figmaScope": "FRAME_FILL, SHAPE_FILL, TEXT_FILL" },
      "brand-hover": { "$value": "{wp.preset.color.brand-hover}", "wpVar": "--wp--preset--color--brand-hover", "figmaScope": "FRAME_FILL, SHAPE_FILL" },
      "secondary": { "$value": "{wp.preset.color.secondary}", "wpVar": "--wp--preset--color--secondary", "figmaScope": "FRAME_FILL, SHAPE_FILL, TEXT_FILL" },
      "secondary-hover": { "$value": "{wp.preset.color.secondary-hover}", "wpVar": "--wp--preset--color--secondary-hover", "figmaScope": "FRAME_FILL, SHAPE_FILL" },
      "background": { "$value": "{wp.preset.color.background}", "wpVar": "--wp--preset--color--background", "figmaScope": "FRAME_FILL" },
      "foreground": { "$value": "{wp.preset.color.card-text}", "wpVar": "--wp--preset--color--card-text", "figmaScope": "TEXT_FILL" },
      "muted": { "$value": "{wp.preset.color.muted}", "wpVar": "--wp--preset--color--muted", "figmaScope": "TEXT_FILL" },
      "card": { "$value": "{wp.preset.color.card-base}", "wpVar": "--wp--preset--color--card-base", "figmaScope": "FRAME_FILL" },
      "card-border": { "$value": "{wp.preset.color.card-border}", "wpVar": "--wp--preset--color--card-border", "figmaScope": "STROKE_COLOR" },
      "border": { "$value": "{wp.preset.color.border}", "wpVar": "--wp--preset--color--border", "figmaScope": "STROKE_COLOR" },
      "text-on-brand": { "$value": "{wp.preset.color.text-on-brand}", "wpVar": "--wp--preset--color--text-on-brand", "figmaScope": "TEXT_FILL" },
      "text-on-secondary": { "$value": "{wp.preset.color.text-on-secondary}", "wpVar": "--wp--preset--color--text-on-secondary", "figmaScope": "TEXT_FILL" },
      "base": { "$value": "{wp.preset.color.base}", "wpVar": "--wp--preset--color--base", "figmaScope": "FRAME_FILL, SHAPE_FILL" }
    },

    "radius": {
      "button": { "$value": "{primitives.radius.lg}", "figmaPx": "6px", "wpVar": "--wp--custom--button--border-radius" },
      "card": { "$value": "{primitives.radius.xl}", "figmaPx": "8px", "wpVar": "--wp--custom--card--border-radius" },
      "input": { "$value": "{primitives.radius.md}", "figmaPx": "4px", "wpVar": "--wp--custom--input--border-radius" },
      "badge": { "$value": "{primitives.radius.full}", "figmaPx": "9999px", "wpVar": "--wp--custom--badge--border-radius" },
      "modal": { "$value": "{primitives.radius.2xl}", "figmaPx": "12px", "wpVar": "--wp--custom--modal--border-radius" }
    },

    "shadow": {
      "card": { "$value": "{primitives.shadow.sm}", "wpVar": "--wp--custom--card--shadow" },
      "card-hover": { "$value": "{primitives.shadow.md}", "wpVar": "--wp--custom--card--shadow-hover" },
      "button": { "$value": "{primitives.shadow.sm}", "wpVar": "--wp--custom--button--shadow" },
      "dropdown": { "$value": "{primitives.shadow.lg}", "wpVar": "--wp--custom--dropdown--shadow" },
      "modal": { "$value": "{primitives.shadow.xl}", "wpVar": "--wp--custom--modal--shadow" }
    },

    "layout": {
      "content-width": { "$value": "{primitives.layoutSize.5xl}", "px": "1024px", "wpVar": "--wp--style--global--content-size" },
      "wide-width": { "$value": "90rem", "px": "1440px", "wpVar": "--wp--style--global--wide-size" },
      "prose-width": { "$value": "{primitives.layoutSize.2xl}", "px": "672px" }
    }
  },

  "components": {
    "button": {
      "bg": { "$value": "{semantic.color.brand}", "mapsTo": "color-brand", "wpVar": "--wp--custom--button--background" },
      "bg-hover": { "$value": "{semantic.color.brand-hover}", "mapsTo": "color-brand-hover", "wpVar": "--wp--custom--button--background-hover" },
      "text": { "$value": "{semantic.color.text-on-brand}", "mapsTo": "color-text-on-brand", "wpVar": "--wp--custom--button--text" },
      "border-radius": { "$value": "{semantic.radius.button}", "mapsTo": "radius-button", "wpVar": "--wp--custom--button--border-radius" },
      "padding-x": { "$value": "{primitives.spacing.5}", "mapsTo": "spacing-5", "wpVar": "--wp--custom--button--spacing--horizontal" },
      "padding-y": { "$value": "{primitives.spacing.2.5}", "mapsTo": "spacing-2.5", "wpVar": "--wp--custom--button--spacing--vertical" },
      "height-sm": { "$value": "{primitives.size.10}", "px": "40px" },
      "height-md": { "$value": "{primitives.size.12}", "px": "48px" },
      "height-lg": { "$value": "{primitives.size.14}", "px": "56px" }
    },

    "card": {
      "bg": { "$value": "{semantic.color.card}", "mapsTo": "color-card", "wpVar": "--wp--preset--color--card-base" },
      "border": { "$value": "{semantic.color.card-border}", "mapsTo": "color-card-border", "wpVar": "--wp--preset--color--card-border" },
      "text": { "$value": "{semantic.color.foreground}", "mapsTo": "color-card-foreground", "wpVar": "--wp--preset--color--card-text" },
      "border-radius": { "$value": "{semantic.radius.card}", "mapsTo": "radius-card", "wpVar": "--wp--custom--card--border-radius" },
      "padding": { "$value": "{primitives.spacing.6}", "mapsTo": "spacing-6", "wpVar": "--wp--custom--card--spacing" },
      "shadow": { "$value": "{semantic.shadow.card}", "mapsTo": "shadow-card", "wpVar": "--wp--custom--card--shadow" }
    },

    "input": {
      "bg": { "$value": "{semantic.color.background}", "wpVar": "--wp--custom--input--background" },
      "border": { "$value": "{semantic.color.border}", "wpVar": "--wp--custom--input--border" },
      "border-radius": { "$value": "{semantic.radius.input}", "mapsTo": "radius-input", "wpVar": "--wp--custom--input--border-radius" },
      "padding-x": { "$value": "{primitives.spacing.3}", "mapsTo": "spacing-3", "wpVar": "--wp--custom--input--spacing--horizontal" },
      "padding-y": { "$value": "{primitives.spacing.2}", "mapsTo": "spacing-2", "wpVar": "--wp--custom--input--spacing--vertical" },
      "height": { "$value": "{primitives.size.10}", "px": "40px" }
    }
  },

  "$figma": {
    "$description": "Figma-specific configuration",
    "collections": [
      {
        "name": "Primitives",
        "modes": ["default"],
        "variables": ["spacing", "radius", "shadow", "duration", "ease", "lineHeight", "fontWeight"]
      },
      {
        "name": "Typography",
        "modes": ["majorSecond", "majorThird"],
        "variables": ["fontSize"]
      },
      {
        "name": "Viewport",
        "modes": ["mobile", "desktop"],
        "variables": ["fontSize.*.mobile", "fontSize.*.desktop"]
      },
      {
        "name": "Layout",
        "modes": ["default"],
        "variables": ["layoutSize", "size"]
      },
      {
        "name": "Semantic",
        "modes": ["light"],
        "variables": ["semantic.*"]
      },
      {
        "name": "Components",
        "modes": ["default"],
        "variables": ["components.*"]
      }
    ],
    "scopes": {
      "color": {
        "brand": ["FRAME_FILL", "SHAPE_FILL", "TEXT_FILL"],
        "background": ["FRAME_FILL"],
        "text": ["TEXT_FILL"],
        "border": ["STROKE_COLOR"]
      }
    }
  }
}
